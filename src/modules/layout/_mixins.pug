include widgets/_header
include widgets/_footer
include widgets/_capture
include widgets/_capture-form
include widgets/_director
include widgets/_services
include widgets/_service-kind
include widgets/_contacts
include widgets/_questions
//- popup
mixin popup()
	.popup.uk-flex-top(data-uk-modal)&attributes(attributes)
		.popup__body.uk-modal-dialog.uk-modal-body.uk-margin-auto-vertical
			button.popup__close.uk-modal-close-default(type="button" data-uk-close)
			block

//- Slider dots and navigation

mixin slider-dots()
	ul.uk-slider-nav.uk-dotnav.slidedots&attributes(attributes)
mixin slideshow-dots()
	ul.uk-slideshow-nav.uk-dotnav.slidedots&attributes(attributes)

mixin slider-nav()
	nav.slidenav&attributes(attributes)
		a.slidenav-btn.slidenav-prev(href="#" data-uk-slider-item="previous")
			+icon('angle-left')
		a.slidenav-btn.slidenav-next(href="#" data-uk-slider-item="next")
			+icon('angle-right')

mixin slideshow-nav()
	nav.slidenav&attributes(attributes)
		a.slidenav-btn.slidenav-prev(href="#" data-uk-slideshow-item="previous")
			+icon('angle-left')
		a.slidenav-btn.slidenav-next(href="#" data-uk-slideshow-item="next")
			+icon('angle-right')

//- APPNAV
mixin appnav(first, second = null)
	nav.appnav&attributes(attributes)
		a(href="/") Главная 
		case first 
			when 'О нас'
				a(href=aboutPageLink) - О нас
			when 'Услуги'
				a(href=servicesPageLink) - Услуги
			when 'Стоимость ремонта'
				a(href=repaircostPageLink) - Стоимость ремонта
			when 'Как мы работаем'
				a(href=howweworkPageLink) - Как мы работаем
			when 'Контакты'
				a(href=contactsPageLink) - Контакты
		if second != null 
			a= ' - ' + second

